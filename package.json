{
  "name": "favalid",
  "description": "validator framework",
  "version": "0.5.0",
  "main": "lib/main.js",
  "repository": "akito0107/favalid",
  "author": {
    "name": "Akito Ito",
    "email": "akito0107@gmail.com"
  },
  "license": "apache-2.0",
  "scripts": {
    "test": "jest ./src",
    "test:watch": "jest --watch ./src",
    "test:cover": "jest --coverage ./src",
    "lint": "tslint ./src/**/*.ts",
    "lint:fix": "tslint --fix",
    "install:type": "flow-typed install",
    "babel": "babel src --out-dir lib --copy-files"
  },
  "dependencies": {
    "@types/power-assert": "^1.5.0",
    "locale-id": "^1.0.2",
    "locale2": "^2.2.0",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.0",
    "@types/ramda": "^0.25.35",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^23.0.0",
    "babel-plugin-add-module-exports": "^0.3.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-jest": "^21.14.0",
    "eslint-plugin-prettier": "^2.6.0",
    "flow-bin": "^0.76.0",
    "flow-typed": "^2.3.0",
    "jest": "^22.4.0",
    "power-assert": "^1.4.4",
    "prettier": "^1.10.2",
    "prettier-eslint": "^8.8.1",
    "regenerator-runtime": "^0.12.0",
    "ts-jest": "^23.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.9.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ],
    "testPathIgnorePatterns": ["/node_modules/", "helper.ts"]
  }
}
